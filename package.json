{
  "name": "vscode-tunnel-action",
  "version": "1.0.0",
  "description": "GitHub Action to download VS Code CLI and start a code tunnel",
  "main": "dist/action.js",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "lint": "tsc --noEmit",
    "build": "esbuild src/action.ts --outfile=dist/action.js --bundle --format=cjs --platform=node --target=node20",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@actions/cache": "^5.0.1",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "github-actions",
    "vscode",
    "tunnel",
    "remote",
    "cli"
  ]
}